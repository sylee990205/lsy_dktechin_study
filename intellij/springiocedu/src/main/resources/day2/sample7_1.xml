<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="developer" class="day2.sample7.Developer">
	<constructor-arg value="dooly"/>
	<constructor-arg value="1500000"/> <!-- Developer가 가진 생성자 중 argument 2개인 생성자를 호출한다는 것을 알 수 있음 -->
	<property name="dept"   value="Development 1 Team"/>
</bean>

<bean id="engineer" class="day2.sample7.Engineer">
	<constructor-arg   value="duke"/>
	<constructor-arg   value="2500000"/>
	<property name="dept"   value="Technology 1 Team"/>	
</bean>

</beans>
